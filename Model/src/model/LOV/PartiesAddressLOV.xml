<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PartiesAddressLOV"
  Version="12.2.1.22.48"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="model.LOV.PartiesAddressLOVRowImpl"
  ComponentClass="model.LOV.PartiesAddressLOVImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.PartiesAddressLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_PARTY_ID"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <Variable
    Name="P_ORG_ID"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT DISTINCT hp.PARTY_ID,         
            hp.PARTY_NAME,           
            hl.ADDRESS1 AS ADDRESS     
FROM hz_parties hp     
    ,hz_party_sites hps,     
     hz_cust_accounts hca,     
     hz_cust_acct_sites_all hcas,     
     HZ_LOCATIONS HL     
WHERE hp.party_id = hca.party_id     
AND hp.party_id = hps.party_id     
AND hcas.party_site_id = hps.party_site_id     
AND HL.LOCATION_ID = hps.LOCATION_ID     
AND hca.cust_account_id = hcas.cust_account_id     
AND hp.STATUS = 'A'     
AND hca.STATUS = 'A'     
AND hcas.STATUS = 'A'     
AND hp.PARTY_ID = :P_PARTY_ID    
AND hcas.org_id=:P_ORG_ID]]></SQLQuery>
  <ViewAttribute
    Name="PartyId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="LONGINT"
    AliasName="PARTY_ID"
    Expression="PARTY_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="PartyName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="360"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARTY_NAME"
    Expression="PARTY_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="360"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="25"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Address"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1260"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ADDRESS"
    Expression="ADDRESS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1260"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="25"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
