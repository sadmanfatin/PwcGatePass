PWC_GATE_PASS_HEADER GPH,           
PWC_GATE_PASS_LINES GPL,        
PWC_GATE_PASS_DETAILS GPD           
WHERE GPH.HEADER_ID = GPL.HEADER_ID        
AND   GPL.LINE_ID   = GPD.LINE_ID         
/*and GPH.GATE_PASS_CLASS = :P_GPT      */      
